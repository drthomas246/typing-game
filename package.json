    "@vitest/coverage-v8": "^3.2.4",
    "fake-indexeddb": "^6.2.2",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "react": "18.2.0",
