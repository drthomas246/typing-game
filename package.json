{
  "name": "typing-game",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "ladle": "npx ladle serve",
    "ladle:build": "npx ladle build",
    "ladle:preview": "npx ladle preview",
    "test": "vitest",
