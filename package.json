    "dexie-react-hooks": "^4.2.0",
    "framer-motion": "11.11.17",
    "howler": "^2.2.4",
    "konva": "^9.3.22",
    "next-themes": "^0.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-konva": "^18.2.14",
    "use-image": "^1.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@csstools/css-parser-algorithms": "^3.0.5",
